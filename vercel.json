{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    { "source": "http://memelist.fun/:path*", "destination": "https://memelist.fun/:path*", "permanent": true },
    { "source": "http://www.memelist.fun/:path*", "destination": "https://memelist.fun/:path*", "permanent": true },
    { "source": "https://www.memelist.fun/:path*", "destination": "https://memelist.fun/:path*", "permanent": true }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
